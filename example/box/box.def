unit box;

data Box {
    x: i32,
    y: i32,
    w: i32,
    h: i32
}

module BoxModule {
    new(x: i32, y: i32, w: i32, h: i32) -> Box {
        @return box

        @spawn box: Box {
            use x, y, w, h;
        }
    }

    move(box: Box, dx: i32, dy: i32) -> Box {
        @return movedBox

        @spawn movedBox: Box {
            use box;
            use dx, dy;
        }
    }

    changeSize(box: Box, w: i32, h: i32) -> Box {
        @return changedBox

        @spawn changedBox: Box {
            use box;
            use w, h;
        }
    }
}
