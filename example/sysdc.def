data String {}

data SysDCSystem {}

data TokenList {}

module Compiler {
    compiile(program: String) -> SysDCSystem {
        @return system

        @spawn system: SysDCSystem {
            use program;
            let tokens = Tokenizer.tokenize(program);
            let system = Parser.parse(tokens);
            return system;
        }
    }
}

module Tokenizer {
    tokenize(program: String) -> TokenList {
        @return tokens

        @spawn tokens: TokenList {
            use program;
        }
    }
}

module Parser {
    parse(tokens: TokenList) -> SysDCSystem {
        @return system

        @spawn system: SysDCSystem {
            use tokens;
        }
    }
}
